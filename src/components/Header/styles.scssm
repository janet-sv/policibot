@import "~sass/utils/index";

.container {
  background: $primary;
}

.header {
  align-items: center;
  display: flex;
  justify-content: space-between;
  margin: auto;
  max-width: 900px;
  padding: 30px;
}

.logo-container {}

.logo-wrapper {
  display: flex;
  max-width: 200px;
  align-items: center;
}

.links {
  display: flex;
}

.link {
  font-size: 17px;
  font-weight: 500;
  padding: 10px 0;
  margin-left: 30px;
}