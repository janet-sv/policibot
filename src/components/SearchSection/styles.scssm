@import "~sass/utils/index";

.search {
  background: $primary;
}

.content {
  margin: auto;
  display: flex;
  align-items: center;
  justify-content: space-between;
  max-width: 900px;
  padding: 50px;
}

.description {
  color: $white;
  font-size: 30px;
  font-weight: 600;
}

.block {
  display: block;
}

.button-wrapper {}

.button {
  background: $secondary;
  color: $white;
  padding: 15px 25px;
  border-radius: 30px;
  font-weight: 600;
  min-width: 250px;
  text-align: center;
}
